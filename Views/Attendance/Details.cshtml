@model WebAppMVC.Models.Attendance

@{
  ViewData["Title"] = "Detail Absensi";
}
<h2>Detail Absensi</h2>
<div>
  <h4>@ViewBag.Message</h4>
  <hr />
  <dl class="row">
    <dt class="col-sm-2">
      Participant
    </dt>
    <dd class="col-sm-10">
      @Model.Participant
    </dd>
    <dt class="col-sm-2">
      Date
    </dt>
    <dd class="col-sm-10">
      @Model.Date
    </dd>
    <dt class="col-sm-2">
      Status
    </dt>
    <dd class="col-sm-10">
      @Model.Status
    </dd>
    <dt class="col-sm-2">
      Notes
    </dt>
    <dd class="col-sm-10">
      @Model.Notes
    </dd>
  </dl>
</div>
<div>
  <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Edit</a> 
  <a class="btn btn-secondary" asp-action="Index">Kembali ke Daftar</a>
</div>